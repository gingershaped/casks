{
  "multipart": [
    {
      "apply": {
        "model": "casks:block/crimson_cask_open"
      },
      "when": {
        "open": "true"
      }
    },
    {
      "apply": {
        "model": "casks:block/crimson_cask"
      },
      "when": {
        "open": "false"
      }
    },
    {
      "apply": {
        "model": "casks:block/cask_frame"
      },
      "when": {
        "above_campfire": "true"
      }
    }
  ]
}